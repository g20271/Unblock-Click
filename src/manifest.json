{
    "manifest_version": 3,
    "name": "Unblock Click",
    "version": "1.0",
    "description": "Applies custom CSS to images within article__wrap_img class.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["styles.css"],
        "js": ["content.js"]
      }
    ]
  }
  