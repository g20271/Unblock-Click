{
    "manifest_version": 3,
    "name": "Unblock Click",
    "version": "1.0",
    "description": "Applies custom CSS to images within article__wrap_img class.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["styles.css"],
        "js": ["content.js"]
      }
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "{3c08317a-95fa-df06-eb3e-90def520c4f3}",
        "strict_min_version": "120.0"
      }
    }
    
  }
  